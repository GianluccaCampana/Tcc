<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.blacaleletrica" >


    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >


        <activity android:name=".DeletarConta" android:screenOrientation="portrait" android:parentActivityName=".DeletarConta"/>
        <activity android:name=".EsqueceuSenha" android:screenOrientation="portrait" android:parentActivityName=".EsqueceuSenha"/>
        <activity android:name=".Cadastro" android:screenOrientation="portrait" android:parentActivityName=".Cadastro"/>
        <activity android:name=".Nbr"  android:screenOrientation="portrait" android:parentActivityName=".Nbr"/>
        <activity android:name=".Configuracao" android:screenOrientation="portrait" android:parentActivityName=".Configuracao"/>
        <activity android:name=".Luminaria"  android:screenOrientation="portrait" android:parentActivityName=".Luminaria"/>
        <activity android:name=".Tomadas" android:screenOrientation="portrait" android:parentActivityName=".Tomadas" />
        <activity android:name=".TelaPrincipal" android:screenOrientation="portrait"  />
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>